    <!-- Pricing Section -->
    <section id="pricing" class="pricing section light-background">

      <!-- Section Title -->
      <div class="container section-title" data-aos="fade-up">
        <h2>Planos e preços</h2>
        <p>Encontre um plano que atenda o seu estabelecimento de forma completa</p>
      </div><!-- End Section Title -->

      <div class="container" data-aos="fade-up" data-aos-delay="100">

        <div class="row g-4 justify-content-center">
          
          {% set popular = '' %}
          {% set popularDiv = '' %}
          {% set popularBotao = 'btn-primary' %}
        {% for row in planos %}
            {% if row.trial == '1' %}
                {% set popularDiv = '<div class="popular-badge">Recomendado</div>' %}
                {% set popular = 'popular' %}
                {% set popularBotao = 'btn-light' %}
            {% endif %}
          <!-- Basic Plan -->
          <div class="col-lg-4" data-aos="fade-up" data-aos-delay="100">
            <div class="pricing-card {{popular}}">
                {{ popularDiv|raw }}
              <h3>{{row.titulo}}</h3>
              <div class="price">
                <span class="amount">
                              {{ row.valor|formataValorEmReal }}</span>
                <span class="period">/ mês</span>
              </div>
              

              <h4>O que vem no plano:</h4>
                {{ row.descricao|listaPlano|raw }}

              <a href="{{ url('landing_cadastro', {'planoId':row.id}) }}" class="btn {{popularBotao}}">
                 Adquirir plano
                <i class="bi bi-arrow-right"></i>
              </a>
            </div>
          </div>
          {% set popularBotao = 'btn-primary' %}
          {% set popular = '' %}
          {% set popularDiv = '' %}
            {% endfor %}


        </div>

      </div>

    </section><!-- /Pricing Section -->



